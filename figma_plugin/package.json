{
  "name": "dscompiler",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "bundle": "./node_modules/.bin/esbuild src/code.ts --bundle --outfile=dist/code.js",
    "bundle_ui": "./node_modules/.bin/esbuild src/ui.ts --bundle --outfile=dist/ui.js && node scripts/buildui.mjs",
    "linter": "npx eslint .",
    "linter_fixer": "npx eslint --fix .",
    "repl": "npx ts-node -r tsconfig-paths/register",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json",
    "watch": "parallel --ungroup 'npm run' ::: watch_typecheck watch_bundle",
    "watch_bundle": "npm run bundle -- --watch=forever",
    "watch_test": "npm run test -- --watch",
    "watch_typecheck": "npm run typecheck -- --watch --preserveWatchOutput"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@figma/plugin-typings": "^1.64.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "esbuild": "0.17.19",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "liquidjs": "^10.8.2",
    "lodash": "^4.17.21"
  }
}
